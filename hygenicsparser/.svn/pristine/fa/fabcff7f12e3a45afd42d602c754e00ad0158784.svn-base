public class NoDataException extends Exception {

	/**
	 * id
	 */
	private static final long serialVersionUID = 2515331882751668217L;

	public NoDataException() {
		super("No Data From Table");
	}

	public NoDataException(String e) {
		super("No Data From Table" + e);
	}

}
