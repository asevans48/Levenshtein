package com.hygenics.exceptions;

public class RepetitiveErrorException extends Exception {

	/**
	 * 
	 */
	private static final long serialVersionUID = -6905752233582896620L;

	public RepetitiveErrorException() {
		super("Repetitive Failures. Task will terminate!");
	}

	public RepetitiveErrorException(String message) {
		super("Repetitive Failures. Task will terminate!\n" + message);
	}
}
